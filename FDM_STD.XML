<!-- This file is used to define tools for the Additive Alliance, LLC 4X3D 3D printer -->
<!-- This file MUST be present in the 4X3D/TOOLS directory for the machine to operate -->
<!-- Tool names MUST BE UNIQUE.  Duplication will give unpredictable results -->
<TOOL>
    <FDM_STD>
	<DESC>Fused deposition modeler</DESC>
	<MANF>Additive Alliance, LLC</MANF>
	<TYPE>ADDITIVE</TYPE>
	<CLASS>FDM</CLASS>
	<PWR48V>TRUE</PWR48V>
	<PWR24V>TRUE</PWR24V>
	<TEMPSENSE>THERMISTOR</TEMPSENSE>
	<MAX_TEMP>300.0</MAX_TEMP>
	<SS_TEMP_ERR>15.0</SS_TEMP_ERR>
	<TIME_PER_C>10.0</TIME_PER_C>
	<KP>4.00</KP>
	<KD>6.00</KD>
	<KI>0.05</KI>
	<STEPPER>1</STEPPER>
	<STPR_DV_RATIO>1.507</STPR_DV_RATIO>
	<SPIBUS>FALSE</SPIBUS>
	<1WIRE>TRUE</1WIRE>
	<PWM_AT_TOOL>FALSE</PWM_AT_TOOL>
	<LIMITSW>FALSE</LIMITSW>
	<X_OFFSET>17.000</X_OFFSET>
	<Y_OFFSET>1.200</Y_OFFSET>
	<TIP_DN_POS>28.500</TIP_DN_POS>
	<WEIGHT>450.0</WEIGHT>
	<OPERATIONS>
	    <ADD_MODEL_MATERIAL>
		<GEOMETRY>SLICE</GEOMETRY>
		<SEQUENCE>MDL_LAYER_1,MDL_OFFSET,MDL_BORDER,MDL_FILL,MDL_LOWER_CO,MDL_UPPER_CO,INT_FILL,INT_LAYER_1,INT_LOWER_CO,INT_UPPE_CO</SEQUENCE>
	    </ADD_MODEL_MATERIAL>
	    <ADD_SUPPORT_MATERIAL>
		<GEOMETRY>SLICE</GEOMETRY>
		<SEQUENCE>SPT_LAYER_1,SPT_BORDER</SEQUENCE>
	    </ADD_SUPPORT_MATERIAL>
	    <ADD_BASE_LAYER>
		<GEOMETRY>SLICE</GEOMETRY>
		<SEQUENCE>BASELYR,PLATFORMLYR1,PLATFORMLYR2</SEQUENCE>
	    </ADD_BASE_LAYER>
	</OPERATIONS>
    </FDM_STD>
</TOOL>
